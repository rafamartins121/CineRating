@model IEnumerable<CineRating.Models.ApplicationUser>

@{
    ViewBag.Title = "Zona de Administração";
}

<h2>Zona de Administração</h2>


<table class="table">
    <tr>
        <th>
            Nome de Utilizador
        </th>
        <th>

        </th>
    </tr>

    @foreach (var item in Model)
    {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.UserName)
            </td>
            <td>
                @Html.ActionLink("Editar", "Edit", new { id = item.Id })
        </tr>
    }

</table>
